{
    "env": {
      "node": true,
      "commonjs": true,
      "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-unused-vars":"warn",
        "no-undef":"warn",
        "no-underscore-dangle": 0,
        "max-len": 0
    }
  }